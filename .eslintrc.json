{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
    "extends": "prettier",
    "plugins": [
        "prettier"
      ],
    "rules": {
        "no-duplicate-case":"error",
        "no-empty":"error",
        "no-extra-semi":"error",
        "no-func-assign": "error",
        "curly": "error",
        "dot-notation": "error",
        "no-empty-function": "error",
        "no-unused-vars": "error",
        "camelcase": "error"
    }
}
